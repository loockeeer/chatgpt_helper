{
  "compilerOptions": {
    "moduleResolution": "node",
    "rootDir": "src",
    "module": "node16",
    "target": "es2020",
    "outDir": "dist",
    "lib": ["es2020"],
    "strict": true,
    "sourceMap": true,
    "declaration": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}